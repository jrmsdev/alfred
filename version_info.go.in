package alfred

import (
	"fmt"
)

const (
	vMajor = "{{.VMajor}}"
	vMinor = "{{.VMinor}}"
	vPatch = "{{.VPatch}}"
)

func init() {
	version = fmt.Sprintf("%s.%s", vMajor, vMinor)
	if vPatch != "0" {
		version = fmt.Sprintf("%s.%s", version, vPatch)
	}
}
