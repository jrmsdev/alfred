package alfred

import (
	"fmt"
)

const (
	sMajor = "{{.VMajor}}"
	sMinor = "{{.VMinor}}"
	sPatch = "{{.VPatch}}"
)

func init() {
	version = fmt.Sprintf("%s.%s", sMajor, sMinor)
	if sPatch != "0" {
		version = fmt.Sprintf("%s.%s", version, sPatch)
	}
}
