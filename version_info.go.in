package alfred

import (
	"fmt"
)

const (
	VMajor = "{{.VMajor}}"
	VMinor = "{{.VMinor}}"
	VPatch = "{{.VPatch}}"
)

func init() {
	version = fmt.Sprintf("%s.%s", VMajor, VMinor)
	if VPatch != "0" {
		version = fmt.Sprintf("%s.%s", version, VPatch)
	}
}
